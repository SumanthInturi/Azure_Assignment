{
	"name": "Source_Dataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FolderName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": "SampleData.csv",
				"folderPath": {
					"value": "@dataset().FolderName",
					"type": "Expression"
				},
				"container": "adf-assignmt-src-container"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "ID",
				"type": "String"
			},
			{
				"name": "Date",
				"type": "String"
			},
			{
				"name": "FirstName",
				"type": "String"
			},
			{
				"name": "LastName",
				"type": "String"
			},
			{
				"name": "Address",
				"type": "String"
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}